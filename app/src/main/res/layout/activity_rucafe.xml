<?xml version="1.0" encoding="utf-8"?>


<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"


    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".RUCafeController">

    <TextView
        android:id="@+id/cafe"
        android:layout_width="364dp"
        android:layout_height="102dp"
        android:fontFamily="@font/oswald_bold"
        android:paddingLeft="5sp"
        android:paddingRight="5sp"
        android:text="Rutgers Cafe"
        android:textColor="#111111"

        android:textSize="55sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.401" />

    <TextView
        android:id="@+id/textView4"
        android:textSize="16sp"
        android:fontFamily="@font/oswald_bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Store Orders"
        android:textColor="#111111"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.9" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="112dp"
        android:layout_height="23dp"
        android:fontFamily="@font/oswald_bold"
        android:text="Coffee Orders"
        android:textColor="#111111"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.547" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/oswald_bold"
        android:text="Donut Orders"
        android:textColor="#111111"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.22"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.547" />

    <TextView
        android:id="@+id/textView"
        android:textSize="16sp"
        android:fontFamily="@font/oswald_bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Orders Basket"
        android:textColor="#111111"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.22"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.9" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="113dp"
        android:layout_height="154dp"
        android:adjustViewBounds="true"
        android:contentDescription="Store Orders"
        android:onClick="openStoreOrdersView"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:srcCompat="@drawable/orders" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="98sp"
        android:layout_height="155sp"
        android:contentDescription="Orders Basket"
        android:onClick="openOrderingBasketView"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="@+id/textView"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:srcCompat="@drawable/basket"/>

    <ImageView
        android:id="@+id/donutImage"
        android:layout_width="117sp"
        android:layout_height="128sp"
        android:layout_marginBottom="8sp"
        android:contentDescription="Donut Image"
        android:onClick="openDonutView"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:srcCompat="@drawable/donuts" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="113sp"
        android:layout_height="125sp"
        android:layout_marginBottom="8sp"
        android:contentDescription="Coffee Image"
        android:onClick="openCoffeeView"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:srcCompat="@drawable/coffee" />



</androidx.constraintlayout.widget.ConstraintLayout>